<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WpfApplication.ViewModel"
                    xmlns:vm2="clr-namespace:ExcelLikeDataGrid.ViewModel;assembly=ExcelLikeDataGrid"
                    xmlns:view="clr-namespace:WpfApplication.View"
                    xmlns:view2="clr-namespace:ExcelLikeDataGrid.View;assembly=ExcelLikeDataGrid">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomersVM}">
        <view:Customers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MetadataVM}">
        <view:Metadata />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm2:FilterWindowVM}">
        <view2:FilterWindow />
    </DataTemplate>

</ResourceDictionary>