<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WpfApplication.ViewModel"
                    xmlns:view="clr-namespace:WpfApplication.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomersVM}">
        <view:Customers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MetadataVM}">
        <view:Metadata />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FilterVM}">
        <view:FilterWindow />
    </DataTemplate>

</ResourceDictionary>